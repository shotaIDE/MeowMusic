{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["asdf"],
      "labels": ["asdf"]
    },
    {
      "matchPackagePatterns": ["gradle-wrapper"],
      "labels": ["gradle-wrapper"]
    },
    {
      "matchPackagePatterns": ["pyenv"],
      "labels": ["pyenv"]
    },
    {
      "matchPackagePatterns": ["ruby-version"],
      "labels": ["ruby-version"]
    },
    {
      "matchPackagePatterns": ["terraform"],
      "labels": ["terraform"]
    }
  ]
}
